<div class="container">
<br>
<%= form_with(model:@questionpaper, url: questionpapers_path(@questionpaper), method: :post,  id: 'questionpaperForm') do |f| %>
  <%= f.hidden_field :teacher_id, value: @teacher.id %>
  <div>
     <%= f.label :name,"Question Paper Title", class:'form-label' %> <%= f.text_field :name, class:'form-control' %>
  </div>
  <br /> 
    <%= f.submit :submit,class:'btn btn-dark' %>
  </div>
</div>
<% end %>

