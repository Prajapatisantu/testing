<div class="container">
<br>
<%= form_with(model:@questionpaper, url: questionpapers_path(@questionpaper), method: :post) do |f| %>
  <div>
     <%= f.label :name,"Question Paper Title", class:'form-label' %> <%= f.text_field :name, class:'form-control' %>
  </div>
  <br>
  <div>
     <%= f.label :teacher_id,class:'form-label' %>
     <%= f.select :teacher_id,(Teacher.all.map{|e| [e.name, e.id]}),class:'form-control' %>
  </div>
  <br /> 
    <%= f.submit :submit,class:'btn btn-primary' %>
  </div>
</div>
<% end %>

