<div class="container">
<br>
<%= form_with(model:@student) do |f| %>
  <div>
     <%= f.label :name,"Name", class:'form-label' %> 
     <%= f.text_field :name, class:'form-control' %>
  </div>
  <br>
  <div>
    <%= f.submit :submit,class:'btn btn-dark' %>
  </div>
  <br>
<% end %>
</div>
<%#= javascript_pack_tag 'student' %>